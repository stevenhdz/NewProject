@model WebApplication2.Models.Analista

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.IdAnalista)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.RolNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.IdAnalista)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.RolNavigation.Role)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.NombreTecnico">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.NombreTecnico">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.NombreTecnico">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
